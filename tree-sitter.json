{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "cherri",
      "camelcase": "Cherri",
      "title": "Cherri",
      "scope": "source.cherri",
      "file-types": [
        "cherri"
      ],
      "injection-regex": "^cherri$",
      "class-name": "TreeSitterCherri"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Cherri grammar for tree-sitter",
    "authors": [
      {
        "name": "videah",
        "email": "me@videah.net",
        "url": "https://videah.net/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-cherri"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}